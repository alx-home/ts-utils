{
  "name": "@alx-home/ts-utils",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "main.cjs",
  "module": "main.js",
  "types": "./main.d.ts",
  "exports": {
    ".": {
      "import": "./main.js",
      "require": "./main.cjs",
      "types": {
        "default": "./main.d.ts"
      }
    },
    "./*.css": {
      "import": "./*.css",
      "require": "./*.css"
    },
    "./build_template": "./build_template.ts",
    "./tailwind": {
      "types": {
        "default": "./tailwind.config.d.ts"
      },
      "import": "./tailwind.config.js",
      "require": "./tailwind.config.cjs"
    },
    "./images": {
      "types": {
        "default": "./images.d.ts"
      }
    },
    "./images/*": {
      "import": "./images/*",
      "require": "./images/*"
    },
    "./images/*?react": {
      "import": "./images/*",
      "require": "./images/*"
    },
    "./fonts": {
      "types": {
        "default": "./fonts.d.ts"
      }
    },
    "./fonts/*": {
      "import": "./fonts/*",
      "require": "./fonts/*"
    },
    "./pdf": {
      "types": {
        "default": "./pdf.d.ts"
      }
    }
  },
  "files": [
    "../../build/ts-utils/dist"
  ],
  "author": "alex-home",
  "scripts": {
    "watch": "npx tsx build.ts --dev --watch",
    "build": "npx tsx build.ts",
    "build:dev": "npx tsx build.ts --dev",
    "lint": "eslint './src/**/*.{js,mjs,cjs,ts,jsx,tsx}'",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@nabla/vite-plugin-eslint": "2.0.5",
    "js-sha512": "0.9.0",
    "lightningcss": "1.29.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@csstools/postcss-cascade-layers": "5.0.1",
    "@eslint/compat": "1.2.9",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.26.0",
    "@rollup/plugin-babel": "6.0.4",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.3",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "@typescript-eslint/parser": "8.31.1",
    "@vitejs/plugin-react": "4.4.1",
    "autoprefixer": "10.4.21",
    "dotenv-cli": "8.0.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.0.0",
    "magic-string": "0.30.17",
    "npm-watch": "0.13.0",
    "postcss": "8.5.3",
    "rollup-plugin-node-externals": "8.0.0",
    "tailwindcss": "^3.4.17",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1",
    "vite": "6.3.5",
    "vite-node": "3.1.3",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vite-plugin-dts": "4.5.3",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vite-plugin-static-copy": "2.3.1",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ]
}